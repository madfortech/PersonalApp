{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    
    "require": {
        "php": "^8.4",
        "fakerphp/faker": "^1.24.1",
        "guzzlehttp/guzzle": "^7.10.0",
        "ipinfo/ipinfolaravel": "^2.9.0",
        "laravel/framework": "^12.11.0",
        "laravel/sanctum": "^4.2.1",
        "laravel/tinker": "^2.8.2",
        "laraveldaily/laravel-charts": "^0.2.3",
        "nowendwell/laravel-terms": "^2.6.0",
        "nunomaduro/collision": "v8.8.3",
        "overtrue/laravel-like": "^5.4.1",
        "spatie/laravel-backup": "^9.3.7",
        "spatie/laravel-feed": "^4.4.3",
        "spatie/laravel-honeypot": "^4.6.2",
        "spatie/laravel-medialibrary": "^11.17.7",
        "spatie/laravel-permission": "6.24.0",
        "spatie/laravel-sitemap": "^7.3.8",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/robots-txt": "^2.5.3",
        "tonysm/rich-text-laravel": "^3.3.0"
    },
    "require-dev": {
        "laravel/breeze": "^2.3.8",
        "laravel/sail": "^1.51.0",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^4.3.0",
        "pestphp/pest-plugin-laravel": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true
 
}
