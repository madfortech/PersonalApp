{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    
    "require": {
        "php": "^8.1",
        "fakerphp/faker": "^1.23.0",
        "guzzlehttp/guzzle": "^7.8.0",
        "laravel/framework": "^10.3.3",
        "laravel/sanctum": "^3.3.2",
        "laravel/tinker": "^2.8.2",
        "nunomaduro/collision": "v7.6.0",
        "spatie/laravel-backup": "^8.4.0",
        "spatie/laravel-feed": "^4.3",
        "spatie/laravel-honeypot": "^4.3.4",
        "spatie/laravel-ignition": "^2.3.1",
        "spatie/laravel-markdown": "^2.4",
        "spatie/laravel-medialibrary": "^10.15.0",
        "spatie/laravel-permission": "6.1.0",
        "spatie/laravel-sitemap": "^7.0",
        "spatie/laravel-sluggable": "^3.5",
        "spatie/robots-txt": "^2.0"
    },
    "require-dev": {
        "laravel/breeze": "^1.26",
        "laravel/sail": "^1.26.1",
        "mockery/mockery": "^1.6.6",
        "pestphp/pest": "^2.8.1",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true
 
}
