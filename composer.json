{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.12",
        "artesaos/seotools": "^1.2",
        "fakerphp/faker": "^1.23.0",
        "guzzlehttp/guzzle": "^7.7.0",
        "laravel/framework": "^10.13.5",
        "laravel/sanctum": "^3.2.5",
        "laravel/tinker": "^2.8.1",
        "laravel/ui": "^4.2.2",
        "maatwebsite/excel": "^3.1",
        "nunomaduro/collision": "v7.6.0",
        "spatie/laravel-backup": "^8.1.11",
        "spatie/laravel-feed": "^4.2",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-ignition": "^2.1.3",
        "spatie/laravel-medialibrary": "^10.10.1",
        "spatie/laravel-permission": "5.9.1",
        "spatie/laravel-sitemap": "^6.3",
        "spatie/laravel-sluggable": "^3.5"
    },
    "require-dev": {
        "laravel/sail": "^1.22.0",
        "mockery/mockery": "^1.6.2",
        "mrdebug/crudgen": "^1.10",
        "phpunit/phpunit": "^10.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,

    "url": "https://satis.spatie.be"
}
